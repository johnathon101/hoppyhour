<h1>Listing foods</h1>

<table>
  <tr>
    <th>Entree</th>
    <th>Sides</th>
    <th>Daytime</th>
    <th>Days</th>
    <th>Place</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @foods.each do |food| %>
  <tr>
    <td><%= food.entree %></td>
    <td><%= food.sides %></td>
    <td><%= food.daytime %></td>
    <td><%= food.days %></td>
    <td><%= food.place_id %></td>
    <td><%= link_to 'Show', place_food_path(@place.id, food.id) %></td>
    <td><%= link_to 'Edit', edit_place_food_path(@place.id, food.id) %></td>
    <td><%= link_to 'Destroy', place_food_path(@place.id, food.id), method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Food', new_place_food_path(@place.id) %>
