<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @place.name %>
</p>

<p>
  <b>Address:</b>
  <%= @place.address %>
</p>

<p>
  <b>City:</b>
  <%= @place.city %>
</p>

<p>
  <b>State:</b>
  <%= @place.state %>
</p>

<p>
  <b>Zip:</b>
  <%= @place.zip %>
</p>

<p>
  <b>Rating:</b>
  <%= @place.rating %>
</p>

<p>
<%   @place.beers.each do |beer| %>
  <h3><%= beer.name %></h3>
  <h5><%= beer.desc %></h5>
<% end %>
</p>

<p>
<%   @place.foods.each do |food| %>
  <h3><%= food.entree %></h3>
  <h5><%= food.sides %></h5>
  <h5><%= food.days %></h5>
<% end %>
</p> 


<%= link_to 'Edit', edit_place_path(@place) %> |
<%= link_to 'Back', places_path %>
